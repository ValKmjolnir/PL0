program fibonacci;
const index:=30;
var return,i;
procedure fib(x);
var sum;
begin
    sum:=0;
    if x<2 then
        return:=x
    else
    begin
        call fib(x-1);
        sum:=sum+return;
        call fib(x-2);
        sum:=sum+return;
        return:=sum
    end
end
begin
    i:=1;
    while i<=index do
    begin
        call fib(i);
        write(return);
        i:=i+1
    end
end